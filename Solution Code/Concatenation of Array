class Solution {
    public int[] getConcatenation(int[] nums) {
        int numlen = nums.length;
        int[] ans = new int[numlen * 2];

        System.arraycopy(nums,0,ans,0,numlen);
        System.arraycopy(nums,0,ans,numlen,numlen);

        // System.out.println(Arrays.toString(ans));
        return ans;
    }
}
